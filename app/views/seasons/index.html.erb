<h1>Select A Season</h1>
<ul>
    <% @seasons.each do |s| %>
        <li><%= link_to "#{s.year}", season_path(s) %></li>
    <% end %>
</ul>
<br><br>
<% if admin_logged_in? %>
    <%= link_to "Create A New Season", new_season_path %>
<% end %>
<br><br>
<% if admin_logged_in? %>
    <%= link_to "Back to HomePage", administrator_path(current_user.id) %>
<% elsif ref_logged_in? %>
    <%= link_to "Back to Homepage", referee_path(current_user.id) %>
<% elsif coach_logged_in? %>
    <%= link_to "Back to Homepage", coach_path(current_user.id) %>
<% end %>