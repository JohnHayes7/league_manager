<%= form_for(player, url: team_player_path(player.team_id, player)) do |f| %>
    <%= f.label "Name" %>
    <%= f.text_field :name %>
    <%= f.label "Jersey Number" %>
    <%= f.text_field :jersey_number %><br>
    <%= f.label "Email" %>
    <%= f.text_field :email %>
    <%= f.label "Phone_Number" %>
    <%= f.text_field :phone_number %><br>
    <%= f.submit %><br>
<% end %>